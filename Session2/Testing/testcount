import unittest

from reverse import eda_bit

class TestCase(unittest.TestCase):

    def test_1_20(self):
        """0ne through 20 failed"""
        self.assertEqual(eda_bit(1,20), [1,2,'Eda',4,'Bit','Eda',7,8,'Eda','Bit',11,'Eda',13,14,'EdaBit',16,17,'Eda',19,'Bit'])

if __name__ == '__main__':
    unittest.main()